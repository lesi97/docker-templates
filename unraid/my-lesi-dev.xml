<?xml version="1.0"?>
<Container version="2">
  <Name>lesi-dev</Name>
  <Repository>ghcr.io/lesi97/lesi.dev:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>lesi.dev Webpage</Overview>
