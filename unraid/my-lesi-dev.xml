  <Config Name="WebUI Port" Target="8080" Default="1357" Mode="tcp" Description="HTTP port" Type="Port" Display="always" Required="true" Mask="false">1537</Config>
  <Config Name="DATABASE_URL_IPV6" Target="DATABASE_URL_IPV6" Default="" Mode="rw" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="DATABASE_URL" Target="DATABASE_URL" Default="" Mode="rw" Description="" Type="Variable" Display="never" Required="false" Mask="false"></Config>
